config {
    compressCss = 1
    compressJs = 1
    concatenateCss = 1
    concatenateJs = 1
}

[applicationContext matches "/^Development/"]
    config {
        compressCss = 0
        compressJs = 0
        concatenateCss = 0
        concatenateJs = 0
        no_cache = 1
        contentObjectExceptionHandler = 0
    }
[END]
