{
    "name": "slavlee/your-package",
    "type": "typo3-cms-extension",
    "description": "Site Package for TYPO3 for an your website",
    "license": "GPL-2.0-or-later",
	"version": "1.0.0",
    "require": {
        "php": "^8.2",
        "typo3/cms-backend": "^12.4",
		"typo3/cms-frontend": "^12.4",
		"typo3/cms-fluid-styled-content": "^12.4",
        "typo3/cms-core": "^12.4",
		"typo3/cms-rte-ckeditor":"^12.4",
		"netresearch/rte-ckeditor-image": "^12.0"
    },
    "require-dev": {
        "typo3/coding-standards": "^0.7.1"
    },
    "authors": [
        {
            "name": "Kevin Chileong Lee",
            "role": "Developer",
            "email": "info@slavlee.de",
            "homepage": "https://slavlee.de/"
        }
    ],
    "keywords": [
        "typo3",
        "site package"
    ],
    "support": {
        "issues": ""
    },
    "autoload": {
        "psr-4": {
            "Wacon\\YourPackage\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "your_package"
        }
    }
}
